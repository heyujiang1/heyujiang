<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.he.dao.CatagoryMapper">
    <select id="queryCatagory" parameterType="int" resultType="com.he.pojo.Catagory">
        select * from mybatis.category where id=#{id};
    </select>

</mapper>